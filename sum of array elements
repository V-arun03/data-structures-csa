#include <stdio.h>
int main()
{
    int n, i;
    int arr[100];
    int sum = 0;
    printf("Enter the number of elements in the array: ");
    scanf("%d", &n);

    printf("Enter %d elements of the array: \n", n);
    for (i = 0; i < n; i++)
    {
        scanf("%d", &arr[i]);
        sum += arr[i];
    }
    printf("Sum of the elements in the array: %d\n", sum);
    return 0;
}
